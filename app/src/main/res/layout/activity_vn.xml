<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".VnActivity">


    <fragment
        android:id="@+id/fragment_history_paragraph"
        android:name="com.tuannq.tflat.TranslateParagraph"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_translate_paragraph" />

    <fragment
        android:id="@+id/fragment_list"
        android:name="com.tuannq.tflat.Fragment_ListParagraphHistory"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_list" />

</LinearLayout>